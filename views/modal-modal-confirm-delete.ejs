        <style>
            .header-menu,footer {
                display: flex;
                align-items: center;
                width: 100%;
            }

            footer {
              padding: 0 10px;
              background: #ccc;
            }
            
         </style>
    </head>
    <body>
        <%- include('navbar-admin.ejs') %>
        

        <script type="text/javascript">
            $(window).on('load', function() {
                $('#showForm').modal('show');
            });
        </script>
        
        <div class="container-fluid" >
            
            <div class="modal fade" tabindex="-1" data-bs-backdrop="static" id="showForm" aria-hidden="true">
                <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                    <h5 class="modal-title">ยืนยันการลบ</h5>
                    <a href="javascript:history.back()">
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </a>
                    </div>
                    <div class="modal-body">
                        <div>คุณต้องการลบ <%= product_delete.name %> หรือไม่ ?</div>
                        <div class="pt-3"> <img src="/images/products/<%= product_delete.image %>" class="img-thumbnail"> </div>
                    </div>
                    <div class="modal-footer">
                    <a href="javascript:history.back()">
                        <button type="button" class="btn btn-secondary">ไม่ลบ</button>
                    </a>
                    <a href="/delete/<%= product_delete._id %>">
                        <button type="button" class="btn btn-primary">ยืนยัน</button>
                    </a>
                    </div>
                </div>
                </div>
            </div>
        </div>
    </body>
